{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Duragraph Intermediate Representation",
  "type": "object",
  "properties": {
    "project_id": { "type": "string" },
    "assistant_id": { "type": "string" },
    "thread_id": { "type": "string" },
    "run_id": { "type": "string" },
    "user_id": { "type": "string" },
    "nodes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "type": { "type": "string" },
          "depends_on": {
            "type": "array",
            "items": { "type": "string" }
          }
        },
        "required": ["id", "type"]
      }
    }
  },
  "required": ["project_id", "assistant_id", "thread_id", "run_id", "user_id", "nodes"]
}