# Example environment variables for Act
# Copy this file to .env and customize as needed

# Enable debug mode
# ACTIONS_RUNNER_DEBUG=true
# ACTIONS_STEP_DEBUG=true

# Default branch
DEFAULT_BRANCH=main

# Go version
GO_VERSION=1.23

# Node version
NODE_VERSION=20

# Python version
PYTHON_VERSION=3.11

# Database configuration for tests
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_NAME=duragraph_test
DB_SSLMODE=disable

# NATS configuration for tests
NATS_URL=nats://localhost:4222

# API configuration
PORT=8080
HOST=0.0.0.0

# Test flags
RUN_INTEGRATION_TESTS=false

# GitHub Pages configuration
# Set to your repository name if using project pages (https://username.github.io/repo-name/)
# Leave empty for user/org pages (https://username.github.io/)
# GITHUB_PAGES_BASE_PATH=/duragraph

# Build configuration for docs and website
NODE_ENV=production
